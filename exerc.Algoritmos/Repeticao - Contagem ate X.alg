Algoritmo "Contagem ate X"

// Função: Pergunte ao usuario um numero e mostre a contagem até esse valor
// Data: 05/2022

Var
 contador, numero: inteiro

Inicio
 Escreva ("Informe um número: ")
 Leia (numero)
 contador <- 0
 Enquanto (contador <= numero) faca
   Escreval (contador)
   contador <- contador + 1
 FimEnquanto
 Escreval ("Acabou!")
Fimalgoritmo